; $ORIGIN kerberos.org
$TTL 3600
@		IN	SOA	ns1 krbcore-dns.mit.edu. (
			2012030101	; Serial
			604800		; Refresh
			86400		; Retry
			2419200		; Expire
			3600)		; Negative Cache TTL
;
