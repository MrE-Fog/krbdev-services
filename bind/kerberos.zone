@		IN	NS	ns2
@		IN	A	18.9.62.44
ns1		IN	A	18.9.62.44
ns2		IN	A	18.9.62.43
www		IN	A	18.9.62.44
blog		IN	CNAME	www
k5wiki		IN	CNAME	www
; testing subdomain
test	  	60	IN	A	18.3.60.39
www.test	60	IN	A	18.3.60.39
blog.test	60	IN	CNAME	www.test
k5wiki.test	60	IN	CNAME	www.test
